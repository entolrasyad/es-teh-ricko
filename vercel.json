{
  "version": 2,
  "framework": null, 
  "regions": ["sin1"],             
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",      
      "dest": "/public/build/"
    },
    {
      "src": "/(.*)",           
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public"
}